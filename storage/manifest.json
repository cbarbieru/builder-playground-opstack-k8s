			"ports": [
				{
					"name": "http",
					"port": 8549,
					"Protocol": "tcp",
					"HostPort": 0
				},
				{
					"name": "p2p",
					"port": 9003,
					"Protocol": "tcp",
					"HostPort": 0
				},
				{
					"name": "p2p",
					"port": 9003,
					"Protocol": "udp",
					"HostPort": 0
				},
				{
					"name": "metrics",
					"port": 7300,
					"Protocol": "tcp",
					"HostPort": 0
				}
			],
			"node_refs": [
				{
					"service": "el",
					"port_label": "http",
					"protocol": "http",
					"user": ""
				},
				{
					"service": "beacon",
					"port_label": "http",
					"protocol": "http",
					"user": ""
				},
				{
					"service": "rollup-boost",
					"port_label": "authrpc",
					"protocol": "http",
					"user": ""
				}
			],
			"files_mapped": {
				"/data/jwtsecret": "jwtsecret",
				"/data/rollup.json": "rollup.json"
			},
			"volumes_mapped": {
				"/data_db": "data"
			},
			"component_name": "op-node",
			"tag": "v1.13.0-rc.1",
			"image": "us-docker.pkg.dev/oplabs-tools-artifacts/images/op-node",
			"entrypoint": "op-node"
		},
		{
			"name": "op-batcher",
			"args": [
				"--l1-eth-rpc",
				"{{Service \"el\" \"http\" \"http\" \"\"}}",
				"--l2-eth-rpc",
